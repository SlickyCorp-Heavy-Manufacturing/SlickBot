{
  "name": "slick-bot",
  "version": "1.0.0",
  "description": "Discord bot",
  "main": "./dist/index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "test": "jasmine-ts --config=jasmine-ts.json",
    "test:integration": "jasmine-ts --config=jasmine-ts-integration.json",
    "test:coverage": "ts-node node_modules/nyc/bin/nyc.js npm test",
    "start": "node ./dist/index.js"
  },
  "author": "Abe",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "0.7.0",
    "@google-cloud/text-to-speech": "3.4.0",
    "cheerio": "1.0.0-rc.10",
    "discord-ytdl-core": "5.0.4",
    "discord.js": "12.5.3",
    "dotenv": "10.0.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "ffmpeg-static": "4.4.1",
    "fuzzyset.js": "1.0.7",
    "got": "11.8.3",
    "imgflip": "2.0.0",
    "iso-639-1": "2.1.11",
    "lodash": "4.17.21",
    "luxon": "2.0.2",
    "moji-translate": "1.0.8",
    "node-schedule": "2.1.0",
    "pureimage": "0.3.2",
    "rxjs": "7.4.0",
    "soundcloud.ts": "0.4.5",
    "tmp-promise": "3.0.3",
    "yargs": "17.3.1",
    "ytdl-core": "4.9.2"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.30",
    "@types/ffmpeg-static": "3.0.1",
    "@types/fuzzyset.js": "0.0.2",
    "@types/jasmine": "3.10.3",
    "@types/lodash": "4.14.178",
    "@types/luxon": "1.27.1",
    "@types/node-schedule": "1.3.2",
    "@types/tmp": "0.2.3",
    "@types/ws": "7.4.7",
    "@types/yargs": "17.0.8",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.25.4",
    "jasmine": "3.10.0",
    "jasmine-spec-reporter": "7.0.0",
    "jasmine-ts": "0.4.0",
    "nock": "13.2.1",
    "nodemon": "2.0.15",
    "nyc": "15.1.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  }
}
