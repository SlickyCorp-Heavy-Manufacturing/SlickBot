{
  "name": "slick-bot",
  "version": "1.0.0",
  "description": "Discord bot",
  "main": "./dist/index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "test": "jasmine-ts --config=jasmine-ts.json",
    "test:integration": "jasmine-ts --config=jasmine-ts-integration.json",
    "test:coverage": "ts-node node_modules/nyc/bin/nyc.js npm test",
    "start": "node ./dist/index.js"
  },
  "author": "Abe",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "@google-cloud/text-to-speech": "^3.2.1",
    "cheerio": "^1.0.0-rc.9",
    "discord-ytdl-core": "^5.0.3",
    "discord.js": "^12.5.3",
    "dotenv": "^8.6.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "ffmpeg-static": "^4.3.0",
    "fuzzyset.js": "^1.0.6",
    "got": "^11.8.2",
    "imgflip": "^2.0.0",
    "iso-639-1": "^2.1.9",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "moji-translate": "^1.0.8",
    "node-schedule": "^1.3.3",
    "pureimage": "^0.2.7",
    "rxjs": "^6.6.7",
    "soundcloud.ts": "^0.3.9",
    "tmp-promise": "^3.0.2",
    "yargs": "^16.2.0",
    "ytdl-core": "^4.8.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.29",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/fuzzyset.js": "0.0.2",
    "@types/jasmine": "^3.7.6",
    "@types/lodash": "^4.14.170",
    "@types/luxon": "^1.26.5",
    "@types/node-schedule": "^1.3.1",
    "@types/tmp": "^0.2.0",
    "@types/ws": "^7.4.4",
    "@types/yargs": "^15.0.13",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.3",
    "jasmine": "^3.7.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jasmine-ts": "^0.3.3",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "nyc": "15.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2"
  }
}
